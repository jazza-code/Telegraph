<!DOCTYPE html>
<html lang="ar" dir="rtl">
<head>
    <meta http-equiv="content-type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>تلغراف - @ViewData["Title"]</title>
    <link href="https://fonts.googleapis.com/css?family=Amiri&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Cairo&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/font-awesome/css/all.min.css" />
    <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    <link rel="shortcut icon" type="image/x-icon" href="@Url.Content("~/favicon.ico")" />
</head>
<body>
    <header>
        @await Html.PartialAsync("_TopBar.cshtml")
        @await Html.PartialAsync("_HeadNav.cshtml", null, new ViewDataDictionary(ViewData) { { "LoginPage", false }, { "RegiserPage", false } })
        @await Html.PartialAsync("_MainNav.cshtml")
    </header>
    <main role="main" class="container-fluid py-3">
        @RenderBody()
    </main>
    <footer class="footer">
        <div class="container">
            &copy; 2019 - تلغراف - <a asp-area="" asp-page="/Privacy">الخصوصية</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js" asp-append-version="true"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js" asp-append-version="true"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
    @await Html.PartialAsync("_CookieConsentPartial")
</body>
</html>
